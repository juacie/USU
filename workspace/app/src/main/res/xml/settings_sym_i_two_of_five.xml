        android:key="@string/setting_Interleaved_2_of_5"
        android:title="@string/config_Interleaved_2_of_5"
        android:summary="%s" app:useSimpleSummaryProvider="true"/>
    <EditTextPreference
        android:dialogLayout="@layout/pref_dialog_edittext_setting_two_integer"

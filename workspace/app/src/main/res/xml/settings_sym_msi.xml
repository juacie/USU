<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ListPreference
        android:defaultValue="0"
        android:entries="@array/pref_enable_disable_titles"
        android:entryValues="@array/pref_enable_disable_values"
        android:key="@string/setting_MSI"
        android:title="@string/config_MSI"
        android:summary="%s" app:useSimpleSummaryProvider="true"/>
    <EditTextPreference
        android:dialogLayout="@layout/pref_dialog_edittext_setting_two_integer"
        android:dialogMessage="@string/dialogMsg_setting_length"
        android:defaultValue="4"
        android:key="@string/setting_MSI_Length_Parameter30"
        android:title="@string/config_MSI_Length_Parameter30" />
    <EditTextPreference
        android:dialogLayout="@layout/pref_dialog_edittext_setting_two_integer"
        android:dialogMessage="@string/dialogMsg_setting_length"
        android:defaultValue="55"
        android:key="@string/setting_MSI_Length_Parameter31"
        android:title="@string/config_MSI_Length_Parameter31" />

    <ListPreference
        android:defaultValue="0"
        android:entries="@array/pref_msi_check_digit_titles"
        android:entryValues="@array/pref_msi_check_digit_values"
        android:key="@string/setting_MSI_Check_Digit"
        android:title="@string/config_MSI_Check_Digit"
        android:summary="%s" app:useSimpleSummaryProvider="true"/>
    <ListPreference
        android:defaultValue="0"
        android:entries="@array/pref_enable_disable_titles"
        android:entryValues="@array/pref_enable_disable_values"
        android:key="@string/setting_Transmit_MSI_Check_Digit"
        android:title="@string/config_Transmit_MSI_Check_Digit"
        android:summary="%s" app:useSimpleSummaryProvider="true"/>
    <ListPreference
        android:defaultValue="1"
        android:entries="@array/pref_MSI_Check_Digit_Algorithm_titles"
        android:entryValues="@array/pref_MSI_Check_Digit_Algorithm_values"
        android:key="@string/setting_MSI_Check_Digit_Algorithm"
        android:title="@string/config_MSI_Check_Digit_Algorithm"
        android:summary="%s" app:useSimpleSummaryProvider="true"/>
</androidx.preference.PreferenceScreen>